global function FullyHidePlayers
global function FullyShowPlayers

void function FullyHidePlayers()
{
    foreach( entity player in GetPlayerArray() )
    {
        player.MakeInvisible()
    }
}

void function FullyShowPlayers()
{
    foreach( entity player in GetPlayerArray() )
    {
        player.MakeVisible()
    }
}

