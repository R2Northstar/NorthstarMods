untyped
globalize_all_functions

function Chat_NetworkWriteLine(string text) {
    NSChatWriteLine(0, text)
}

function Chat_GameWriteLine(string text) {
    NSChatWriteLine(1, text)
}

function Chat_NetworkWrite(string text) {
    NSChatWrite(0, text)
}

function Chat_GameWrite(string text) {
    NSChatWrite(1, text)
}
